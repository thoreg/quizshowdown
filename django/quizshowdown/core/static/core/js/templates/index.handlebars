<div class="row">
    <div class="col-md-8 col-md-offset-2">
        <div class="row header">
            <a {{action 'addQuizz'}}>
                <img class="add" src="/static/img/add.png" />
            </a>
            Add a question
        </div>
        {{bs-alert message="Welcome to quizshowdown - Nice to see you" type="warning" dismissAfter=2 fade=true}}
        <table class="table highscore">
        <thead>
            <tr>
                <th>Highscore</th>
                <th>Username</th>
            </tr>
        </thead>
        {{#each}}
            <tr>
                <td>{{highscore}}</td>
                <td>{{username}}</td>
            </tr>
        {{/each}}
        </table>
   </div>
</div>
